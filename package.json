{
  "name": "node-ical",
  "version": "0.20.1",
  "main": "node-ical.js",
  "types": "node-ical.d.ts",
  "description": "NodeJS class for parsing iCalendar/ICS files",
  "keywords": [
    "ical",
    "ics",
    "calendar",
    "nodejs"
  ],
  "homepage": "https://github.com/jens-maus/node-ical",
  "author": "Jens Maus <mail@jens-maus.de>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/jens-maus/node-ical.git"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "moment-timezone": "^0.6.0",
    "rrule": "2.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "ansi-regex": ">=6.2.2",
    "diff": ">=8.0.2",
    "dot-prop": ">=9.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "pre-commit": "^1.2.2",
    "set-value": ">=4.0.1",
    "underscore": "1.13.7",
    "vows": "^0.8.2",
    "xml-js": "^1.6.11",
    "braces": "^3.0.3",
    "xo": "~1.2.2"
  },
  "xo": {
    "space": 2,
    "rules": {
      "@stylistic/comma-dangle": "off",
      "@stylistic/function-paren-newline": "off",
      "@stylistic/indent-binary-ops": "off",
      "@stylistic/max-len": "off",
      "@stylistic/multiline-ternary": "off",
      "@stylistic/operator-linebreak": "off",
      "@typescript-eslint/consistent-type-definitions": "off",
      "@typescript-eslint/consistent-type-imports": "off",
      "@typescript-eslint/member-ordering": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/no-unnecessary-boolean-literal-compare": "off",
      "@typescript-eslint/prefer-nullish-coalescing": "off",
      "arrow-body-style": "off",
      "import-x/order": "off",
      "logical-assignment-operators": "off",
      "n/prefer-global/process": "off",
      "unicorn/prefer-includes": "off",
      "n/prefer-promises/fs": "off",
      "no-useless-catch": "off",
      "object-shorthand": "off",
      "prefer-object-has-own": "off",
      "promise/prefer-await-to-then": "off",
      "unicorn/consistent-date-clone": "off",
      "unicorn/consistent-existence-index-check": "off",
      "unicorn/filename-case": "off",
      "unicorn/prefer-at": "off",
      "unicorn/prefer-prototype-methods": "off",
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-ternary": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-typeof-undefined": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-node-protocol": "off",
      "unicorn/prefer-spread": "off",
      "unicorn/numeric-separators-style": "off"
    }
  },
  "pre-commit": [
    "lintfix"
  ],
  "scripts": {
    "test": "xo && vows test/test.js && vows test/test-async.js && vows test/test-fromURL.js && printf \"\\n\"",
    "lint": "xo",
    "lintfix": "xo --fix",
    "precommit": "npm run lint && npm run lintfix",
    "build": "build/updatejson.sh"
  },
  "readmeFilename": "README.md"
}
